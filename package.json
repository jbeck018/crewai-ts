{
  "name": "crewai-ts",
  "version": "0.1.0",
  "description": "TypeScript port of crewAI for agent-based workflows",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./flow": {
      "import": "./dist/flow/index.js",
      "types": "./dist/flow/index.d.ts"
    },
    "./flow/decorators": {
      "import": "./dist/flow/decorators.js",
      "types": "./dist/flow/decorators.d.ts"
    },
    "./cli": {
      "import": "./dist/cli/index.js",
      "types": "./dist/cli/index.d.ts"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "crewai": "./bin/crewai.js"
  },
  "files": [
    "dist",
    "bin"
  ],
  "scripts": {
    "build": "tsc",
    "build:analyze": "tsc && npx bundlesize",
    "build:production": "tsc --project tsconfig.production.json",
    "dev": "tsc --watch",
    "test": "bun test",
    "test:ci": "NODE_OPTIONS='--max-old-space-size=4096' bun test --coverage",
    "test:vitest": "vitest run",
    "test:vitest:watch": "vitest",
    "test:memory": "bun test ./src/memory/*.test.ts",
    "test:knowledge": "bun test ./src/knowledge/*.test.ts",
    "lint": "eslint --ext .ts src",
    "lint:fix": "eslint --ext .ts src --fix",
    "format": "prettier --write 'src/**/*.ts'",
    "type-check": "tsc --noEmit",
    "prepare": "husky install",
    "pre-commit": "lint-staged",
    "pre-push": "npm run type-check && npm run test",
    "cli": "node bin/crewai.js",
    "create-flow": "node bin/crewai.js create-flow",
    "run-flow": "node bin/crewai.js run-flow",
    "plot-flow": "node bin/crewai.js plot-flow"
  },
  "keywords": [
    "crewai",
    "typescript",
    "agents",
    "llm",
    "ai"
  ],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0",
    "bun": ">=1.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^22.14.0",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "bundlesize": "^0.18.2",
    "bun-types": "^1.2.9",
    "eslint": "^8.57.1",
    "expect": "^29.7.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.5.0",
    "prettier": "^3.5.3",
    "typescript": "^5.8.3",
    "vis-network": "^9.1.9",
    "vitest": "^1.6.1"
  },
  "dependencies": {
    "chalk": "^5.4.1",
    "commander": "^11.1.0",
    "gpt-tokenizer": "^2.9.0",
    "jsdom": "^22.1.0",
    "mysql2": "^3.14.0",
    "ora": "^7.0.1",
    "pg": "^8.14.1",
    "uuid": "^11.1.0",
    "zod": "^3.24.2"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "bundlesize": [
    {
      "path": "./dist/**/*.js",
      "maxSize": "500 kB"
    }
  ]
}
